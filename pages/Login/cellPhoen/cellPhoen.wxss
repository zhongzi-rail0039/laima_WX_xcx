/* pages/Login/cellPhoen/cellPhoen.wxss */
page {
  background-color: #ffffff;
}

.left {
  float: left;
}

.right {
  float: right;
}

.cellPhone {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.login-images image {
  width: 160rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
  margin: 160rpx 0 120rpx 0;
}

.cellPhone-conter {
  display: flex;
  flex-direction: column;
}

.cellPhone-input-phone {
  height: 60rpx;
  line-height: 60rpx;
  padding: 10rpx;
  color: #333;
  border-bottom: 1rpx solid #E3E3E3;
}

.cellPhone-input-password {
  height: 60rpx;
  line-height: 60rpx;
  padding: 60rpx 10rpx 4rpx;
  color: #333;
  border-bottom: 1rpx solid #E3E3E3;
}

.cellPhone-input-password .cellPhone-input-passworld-button {
  font-size: 24rpx;
  color: #F8195D;
  border: 1px solid #F8195D;
  border-radius: 100rpx;
  width: 188rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.cellPhone-Login::after {
  border: 0;
}

.cellPhone-Login {
  width: 542rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 100rpx;
  color: #ffffff;
  margin-top: 100rpx;
  background-color: #F8195D;
}