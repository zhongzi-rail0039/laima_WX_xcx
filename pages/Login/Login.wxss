/* pages/Login/Login.wxss */
page {
  background-color: #ffffff;
}

.myLogin {
  display: flex;
  flex-direction: column;
  align-items:center;
}

.myLogin-empty image {
  width: 200rpx;
  height: 200rpx;
  margin-top: 220rpx;
  border-radius: 20rpx;
}

.phonelogin {
  margin-top: 128rpx;
  color: #ffffff;
  background-color: #F8195D;
}

.primary::after {
  border: none;
}

.primary {
  width: 540rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 100rpx;
  font-weight: 600;
}

.myLogin-treaty {
  margin-top: 480rpx;
  font-size: 24rpx;
  color: #9D9D9D;
}

.myLogin-treaty-text {
  color: rgb(111, 140, 197);
}